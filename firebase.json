{
  "database": {
    "rules": "database.rules.json"
  },
  "functions": {
    "source": "HwR_backend"
  },
  "hosting": {
    "site": "lasthome-869f7",
    "public": "public",
    "rewrites": [{
      "source": "/requestsync",
      "function": "requestsync"
    },
    {
      "source": "/login",
      "destination": "/login/login.html"
    }],
    "headers": [{
      "source" : "**/*.@(js|html)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=0"
      } ]
    }]
  }
}
